@model IEnumerable<Mupadoodle1.Models.City>

@{
    ViewBag.Title = "List of Cities";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
    <h2>@ViewBag.Message</h2>
</hgroup>

<table>
    <tr>
        <th>
            Latitude
        </th>
        <th>
           Longitude
        </th>
        <th>
           Name
        </th>
        <th>
           Number of Museums
        </th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.theLat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.theLong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.theName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.museums.Count)
        </td>
    </tr>
}
</table>

<article>
    <p>
        Use this area to provide additional information.
    </p>

    <p>
        Use this area to provide additional information.
    </p>

    <p>
        Use this area to provide additional information.
    </p>
</article>

